<div class="card" >
  <h1 class="card-header">New Challenge</h1>
  <div class="card-body">
    <%= render 'form', project: @project %>
    </div>
  <div class="card-footer text-muted">
    <div class="btn-group" role="group">
      <%= button_to 'View', @project, :class => "btn shiny btn-success" %>
      <%= button_to 'Back', projects_path, :class => "btn shiny btn-yellow" %>
    </div>
  </div>
</div>
