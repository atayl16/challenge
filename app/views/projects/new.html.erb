<div class="card" >
  <h3 class="card-header">
    <div class="fa fa-trophy"></div>
    New Challenge
  </h3>
  <div class="card-body">
    <%= render 'form', project: @project %>
  </div>
  <div class="card-footer text-muted">
    <div class="btn-group" role="group">
      <%= button_to 'View', @project, :class => "btn shiny btn-success" %>
      <%= button_to 'Back', projects_path, :class => "btn shiny btn-yellow" %>
    </div>
  </div>
</div>
